<!DOCTYPE html>
<html class="loading" lang="en" data-textdirection="ltr">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <title>@ViewBag.Title</title>
    <meta name="description" content="IDV - Registration form">
    <meta name="keywords" content="IDV, Pacific, IDV Pacific, registration form">
    <meta name="author" content="EMAS">
    <link rel="shortcut icon" href="~/Image/Logo/Regform.ico" type="image/x-icon">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/vendors/css/vendors.min.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/css/bootstrap-extended.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/css/colors.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/css/components.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/css/core/menu/menu-types/horizontal-menu.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/assets/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/vendors/css/extensions/toastr.css">
    @RenderSection("CSSScript", false)
    <style>
        .BtnNav {
            left: 0;
            position: fixed;
            bottom: 0px;
            width: 100%;
            height: auto;
        }
        .BtnNavSha {
            -webkit-box-shadow: -1px -1px 23px 0px rgba(0, 0, 0, 0.14);
            -moz-box-shadow: -1px -1px 23px 0px rgba(0, 0, 0, 0.14);
            box-shadow: -1px -1px 23px 0px rgba(0, 0, 0, 0.14);
            align-items: center;
            justify-content: left;
        }
        .AUHBNEL {
        }
        @@media screen and (max-width: 576px) {
            .AUHBNEL {
                display: none !important;
            }
        }
        .GIC_LNT_Hide {
        }
    </style>
</head>
<body class="" data-open="hover" data-menu="horizontal-menu" data-col="2-columns">
    <nav class="header-navbar navbar-expand-lg navbar navbar-with-menu navbar-fixed navbar-shadow GIC_LNT_Hide" style="background-color:white !important">
        <div class="navbar-wrapper">
            <div class="navbar-container">
                <div class="form-inline" id="navbar-mobile">
                    <div class="col-lg-4" style="text-align:center">
                        <div class="nav-item d-none d-lg-block" style="pointer-events:none;font-weight:bold;padding:10px"><a class="nav-link">@ViewBag.Header1</a></div>
                    </div>
                    <div class="col-lg-4" style="text-align:center">
                        <img src="@ViewBag.LogoData" style="max-width:100px;height:auto;padding-top:10px;padding-bottom:10px" />
                    </div>
                    <div class="col-lg-4" style="text-align:center">
                        <div class="nav-item d-lg-block" style="pointer-events:none;font-weight:bold;padding:10px"><a class="nav-link">@ViewBag.Header2</a></div>
                    </div>
                </div>
            </div>
        </div>
    </nav>
    <div class="app-content content" style="padding-left:0px;padding-right:0px;margin:0px">
        <div class="content-wrapper" style="margin:0px;padding:10px">
            <div class="content-body" style="padding-left:0px;padding-right:0px;margin:0px;margin-top:10px">
                @RenderBody()
            </div>
        </div>
    </div>
    <div class="BtnNav GIC_LNT_Hide">
        @if (ViewBag.SectionAsPage == "0")
        {
            <div class="col-lg-12" id="Btn_FormController" style="opacity:1;pointer-events:auto">
                <div class="BtnNavSha form-inline" style="width:100%;box-sizing:border-box;background-color:white;min-height:60px;border-radius:10px;font-size:15px;font-weight:normal;font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif">
                    <div style="margin-left:35px">
                        <a href="javascript:void(0)" style="color:#494949" onclick="Submit_Form()"><i class="fa fa-check" style="margin-right:5px"></i><span>Submit Form</span></a>
                    </div>
                    <div style="margin-left:35px" class="AUHBNEL">
                        <a href="javascript:void(0)" style="color:#494949" onclick="Reset_Form()"><i class="fa fa-trash" style="margin-right:5px"></i><span>Clear Form</span></a>
                    </div>
                    <div style="margin-left:35px" class="AUHBNEL">
                        <a href="javascript:void(0)" style="color:#494949" onclick="Reload_Form()"><i class="fa fa-refresh" style="margin-right:5px"></i><span>Reload Form</span></a>
                    </div>
                </div>
            </div>
        }
        <footer class="footer-light navbar-shadow" style="margin-top:20px;padding:10px;background-color:#f8f8f8 !important">
            <p class="clearfix blue-grey lighten-2">
                <span class="float-md-left d-block d-md-inline-block mt-25">@ViewBag.Footer1</span>
                <span class="float-md-right d-block d-md-inline-block mt-25">@ViewBag.Footer2</span>
                <button class="btn btn-primary btn-icon scroll-top" type="button" style="z-index:9999999"><i class="feather icon-arrow-up"></i></button>
            </p>
        </footer>
    </div>
    <script src="~/Theme/Temp1/app-assets/vendors/js/vendors.min.js"></script>
    <script src="~/Theme/Temp1/app-assets/vendors/js/ui/jquery.sticky.js"></script>
    <script src="~/Theme/Temp1/app-assets/js/core/app-menu.js"></script>
    <script src="~/Theme/Temp1/app-assets/js/core/app.js"></script>
    <script src="~/Theme/Temp1/app-assets/js/scripts/footer.js"></script>
    <script src="~/Theme/Temp1/app-assets/vendors/js/extensions/toastr.min.js"></script>
    @RenderSection("JSScript", false)
</body>
</html>