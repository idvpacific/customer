<!DOCTYPE html>
<html class="loading" lang="en" data-textdirection="ltr">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <title>@ViewBag.Title</title>
    <meta name="description" content="IDV - Registration form">
    <meta name="keywords" content="IDV, Pacific, IDV Pacific, registration form">
    <meta name="author" content="EMAS">
    <link rel="shortcut icon" href="~/Image/Logo/Regform.ico" type="image/x-icon">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/vendors/css/vendors.min.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/css/bootstrap-extended.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/css/colors.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/css/components.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/css/core/menu/menu-types/horizontal-menu.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/assets/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Theme/Temp1/app-assets/vendors/css/extensions/toastr.css">
    @RenderSection("CSSScript", false)
    <style>
        .horizontal-menu .header-navbar.navbar-brand-center .navbar-header .navbar-brand .brand-logo {
            background: url( @ViewBag.LogoData );
            background-repeat: no-repeat;
            background-size: 215px 55px;
            background-position: 0px 0px;
            height: 55px;
            width: 215px;
        }
        .main-menu .navbar-header .navbar-brand .brand-logo {
            background: url( @ViewBag.LogoData );
            background-repeat: no-repeat;
            background-size: 169px 40px;
            background-position: 0px 0px;
            height: 40px;
            width: 169px;
        }
        .GIC_LNT_Hide{
        }
    </style>
</head>
<body class="horizontal-layout horizontal-menu 1-columns navbar-floating" data-open="hover" data-menu="horizontal-menu" data-col="2-columns">
    <nav class="header-navbar navbar-expand-lg navbar navbar-with-menu navbar-fixed navbar-shadow navbar-brand-center GIC_LNT_Hide">
        <div class="navbar-header d-xl-block d-none">
            <ul class="nav navbar-nav flex-row">
                <li class="nav-item">
                    <a class="navbar-brand" href="@ViewBag.MasterUserWebsiteUrl" target="_blank">
                        <div class="brand-logo"></div>
                    </a>
                </li>
            </ul>
        </div>
        <div class="navbar-wrapper">
            <div class="navbar-container content">
                <div class="navbar-collapse" id="navbar-mobile">
                    <div class="mr-auto float-left bookmark-wrapper d-flex align-items-center">
                        <ul class="nav navbar-nav">
                            <li class="nav-item mobile-menu d-xl-none mr-auto">
                                <a class="nav-link nav-menu-main menu-toggle hidden-xs" href="javascript:void(0)">
                                    <i class="ficon feather icon-menu"></i>
                                </a>
                            </li>
                        </ul>
                        <ul class="nav navbar-nav bookmark-icons" style="pointer-events:none">@ViewBag.Header1</ul>
                    </div>
                    <ul class="nav navbar-nav float-right">
                        <li class="nav-item d-none d-lg-block" style="pointer-events:none"><a class="nav-link">@ViewBag.Header2</a></li>
                        <li class="dropdown dropdown-language nav-item">
                            <a class="dropdown-toggle nav-link" id="dropdown-flag" href="javascript:void(0)" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="flag-icon flag-icon-us"></i><span class="selected-language">English</span></a>
                            <div class="dropdown-menu" aria-labelledby="dropdown-flag">
                                <a class="dropdown-item" data-language="en"><i class="flag-icon flag-icon-us"></i> English</a>
                            </div>
                        </li>
                        <li class="nav-item d-none d-lg-block"><a class="nav-link nav-link-expand"><i class="ficon feather icon-maximize"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>
    @if (ViewBag.SectionAsPage == "0")
    {
        <div class="horizontal-menu-wrapper GIC_LNT_Hide" id="Btn_FormController" style="opacity:1;pointer-events:auto">
            <div class="header-navbar navbar-expand-sm navbar navbar-horizontal floating-nav navbar-light navbar-without-dd-arrow navbar-shadow menu-border" role="navigation" data-menu="menu-wrapper">
                <div class="navbar-header">
                    <ul class="nav navbar-nav flex-row">
                        <li class="nav-item mr-auto">
                            <a class="navbar-brand" href="@ViewBag.MasterUserWebsiteUrl" target="_blank">
                                <div class="brand-logo"></div>
                            </a>
                        </li>
                        <li class="nav-item nav-toggle"><a class="nav-link modern-nav-toggle pr-0" data-toggle="collapse"><i class="feather icon-x d-block d-xl-none font-medium-4 primary toggle-icon"></i><i class="toggle-icon feather icon-disc font-medium-4 d-none d-xl-block collapse-toggle-icon primary" data-ticon="icon-disc"></i></a></li>
                    </ul>
                </div>
                <div class="navbar-container main-menu-content" data-menu="menu-container">
                    <ul class="nav navbar-nav" id="main-menu-navigation" data-menu="menu-navigation">
                        <li class="nav-item" data-menu="dropdown">
                            <a class="nav-link" href="javascript:void(0)" data-toggle="dropdown" onclick="Submit_Form()"><i class="fa fa-check"></i><span data-i18n="Dashboard">Submit Form</span></a>
                        </li>
                        <li class="nav-item" data-menu="dropdown">
                            <a class="nav-link" href="javascript:void(0)" data-toggle="dropdown" onclick="Reset_Form()"><i class="fa fa-trash"></i><span data-i18n="Dashboard">Clear Form</span></a>
                        </li>
                        <li class="nav-item" data-menu="dropdown">
                            <a class="nav-link" href="javascript:void(0)" data-toggle="dropdown" onclick="Reload_Form()"><i class="fa fa-refresh"></i><span data-i18n="Dashboard">Reload Form</span></a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    }
    <div class="app-content content" style="padding-left:0px;padding-right:0px">
        <div class="content-overlay"></div>
        <div class="content-wrapper">
            <div class="content-body" style="padding-left:0px;padding-right:0px">
                @RenderBody()
            </div>
        </div>
    </div>
    <footer class="footer footer-light navbar-shadow GIC_LNT_Hide">
        <p class="clearfix blue-grey lighten-2 mb-0">
            <span class="float-md-left d-block d-md-inline-block mt-25">@ViewBag.Footer1</span>
            <span class="float-md-right d-block d-md-inline-block mt-25">@ViewBag.Footer2</span>
            <button class="btn btn-primary btn-icon scroll-top" type="button"><i class="feather icon-arrow-up"></i></button>
        </p>
    </footer>
    <script src="~/Theme/Temp1/app-assets/vendors/js/vendors.min.js"></script>
    <script src="~/Theme/Temp1/app-assets/vendors/js/ui/jquery.sticky.js"></script>
    <script src="~/Theme/Temp1/app-assets/js/core/app-menu.js"></script>
    <script src="~/Theme/Temp1/app-assets/js/core/app.js"></script>
    <script src="~/Theme/Temp1/app-assets/js/scripts/footer.js"></script>
    <script src="~/Theme/Temp1/app-assets/vendors/js/extensions/toastr.min.js"></script>
    @RenderSection("JSScript", false)
</body>
</html>